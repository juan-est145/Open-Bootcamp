{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "API_OpenBootcampDB": "Server=localhost;Database=API_OpenBootcampDB;Trusted_Connection=True;TrustServerCertificate=True"
  },
  "JsonWebTokenKys": {
